 	DEF_MOD("fdp1-0",		 119,	R8A774B1_CLK_S0D1),
+	DEF_MOD("vcplf",		 130,	R8A774B1_CLK_S0D2),
+	DEF_MOD("vdpb",			 131,	R8A774B1_CLK_S0D2),
